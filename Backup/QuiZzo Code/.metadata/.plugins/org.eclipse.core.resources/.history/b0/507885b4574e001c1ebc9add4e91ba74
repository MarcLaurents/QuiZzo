package QuiZzoInc;

import pack.QuiZzoInc;

public class MenuEscolhaPersonagem {
	
	private String idEscolhaPersonagem;
    private String personagemCadastrados;
    private int totalPersonagens;
    private boolean personagemEscolhido = false;

    public void personagemEscolhido() {
	  	String botaoEscolhaPersonagem = "ESCOLHA SEU PERSONAGEM";
	  	
		switch(botaoEscolhaPersonagem) {
		
		case "JAMES RODRIGUES":	
			
			this.statusPersonagem = true;
			
			break;
			
		case "SAMARA CHAMADO":
			
			this.statusPersonagem = true;
			
			break;
		
		}
  }

}

